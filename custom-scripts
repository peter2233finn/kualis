ftp:                    ftp-nmap-scripts        nmap -sV -sC -Pn XXIPXX -p XXPORTXX --script="*ftp*"
omginitialrefs?:        omg-nmap-scan           nmap -sV -sC XXIPXX -p XXPORTXX -Pn
telnet:                 nmap-script-telnet      nmap XXIPXX -p XXPORTXX --script="*telnet*" -Pn
fw1-secureremote:       Banner-grabbed          printf '\x51\x00\x00\x00\x00\x00\x00\x21\x00\x00\x00\x0bsecuremote\x00' | timeout 3 nc -w 1 XXIPXX XXPORTXX
snmp:                   snmp-bulk-walk-v2c      timeout 20 snmpbulkwalk -c public -v2c XXIPXX .
snmp:                   snmp-bulk-walk-v1       timeout 20 snmpbulkwalk -c public -v1 XXIPXX .
ms-sql-s:               ms-sql-nmap-script      nmap XXIPXX -p XXPORTXX -Pn --script="*ms-sql* and not *brute*"
unknown:                unknown-nmap            nmap XXIPXX -p XXPORTXX -Pn -sV -sC -oA enum
