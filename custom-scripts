ssl/http:               curl-to-root            curl -v https://XXIPXX:XXPORTXX 2>&1
isakmp?:                ike-scan                ike-scan XXIPXX --dport XXPORTXX
smtp:                   nmap-smtp-scripts       nmap XXIPXX -p XXPORTXX --script="*smtp*" -Pn
smtp:                   smtp-user-enum          smtp-user-enum -t XXIPXX -p XXPORTXX -U /usr/share/wordlists/common-snmp-community-strings.txt
