http:           gobuster-directory      gobuster dir -u http://XXIPXX:XXPORTXX -w /usr/share/dirb/wordlists/big.txt
http:           curl-to-root            curl -v http://XXIPXX:XXPORTXX 2>&1
ssl/https?:     sslscan                 sslscan XXIPXX:XXPORTXX
ssl/https?:     nikto                   nikto -host https://XXIPXX:XXPORTXX
ssl/https?:     gobuster-directory      gobuster dir -u https://XXIPXX:XXPORTXX -w /usr/share/dirb/wordlists/big.txt
ssl/http:       curl-to-root            curl -v https://XXIPXX:XXPORTXX 2>&1
isakmp?:        ike-scan                ike-scan XXIPXX --dport XXPORTXX
smtp:           nmap-smtp-scripts       nmap XXIPXX -p XXPORTXX --script="*smtp*"
smtp:           smtp-user-enum          smtp-user-enum -t XXIPXX -p XXPORTXX -U /usr/share/wordlists/common-snmp-community-strings.txt
